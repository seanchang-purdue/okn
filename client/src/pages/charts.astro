---
import Layout from "../layouts/Layout.astro";
import ChartApp from "../components/core/ChartApp";
import OknNavbar from "../components/utils/OknNavbar";

const path = Astro.url.pathname;
---

<Layout title="OKN">
  <main
    class="flex flex-col items-center justify-center w-full bg-white dark:bg-gray-900 transition duration-150"
  >
    <OknNavbar client:only="react" path={path} />
    <ChartApp client:only="react" />
  </main>
</Layout>
